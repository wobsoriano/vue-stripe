{
  "name": "vue-stripe",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@10.20.0",
  "description": "Vue components for Stripe.js and Stripe Elements",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-stripe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-stripe.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-stripe/issues",
  "keywords": [
    "vue",
    "stripe",
    "elements"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/react-stripe.d.ts",
      "import": "./dist/index.js"
    },
    "./checkout": {
      "types": "./dist/checkout/index.d.ts",
      "import": "./dist/checkout/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch --onSuccess \"pnpm build:dts\"",
    "build": "tsdown --env.NODE_ENV production",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "pnpm build && changeset publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@stripe/stripe-js": ">=8.0.0 <9.0.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@changesets/cli": "^2.29.7",
    "@stripe/stripe-js": "^8.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.9.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "eslint": "^9.38.0",
    "jsdom": "^27.0.1",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-composable-testing": "^0.2.0"
  }
}
